<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smartmobion.feelhungry"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.smartmobion.feelhungry.app.AppController"
        android:allowBackup="true"
        android:icon="@drawable/app_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.smartmobion.feelhungry.SpashActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.smartmobion.feelhungry.BaseActivity"
            android:label="@string/app_name" />
        <activity android:name="com.smartmobion.feelhungry.MainActivity" />
        <activity android:name="com.smartmobion.feelhungry.HomeActivity" />
        <activity
            android:name="com.smartmobion.feelhungry.Restuarants"
            android:label="@string/title_activity_restuarants"
            android:parentActivityName="com.smartmobion.feelhungry.MainActivity"
            android:theme="@style/AppTheme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.smartmobion.feelhungry.MainActivity" />
        </activity>
    </application>

</manifest>